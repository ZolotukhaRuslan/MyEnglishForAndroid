<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.audioLessonsActivity.AudioLessonMenuActivity">

    <Button
        android:id="@+id/audioLessonOne"
        android:layout_width="0dp"
        android:layout_height="54dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="69dp"
        android:layout_marginEnd="59dp"
        android:onClick="audioLessonOne"
        android:text="Audio Lesson One"
        app:layout_constraintEnd_toStartOf="@+id/audioLessonTwo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/audioLessonTwo"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="54dp"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="@+id/audioLessonOne"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/audioLessonOne"
        app:layout_constraintTop_toTopOf="@+id/audioLessonOne" />
</androidx.constraintlayout.widget.ConstraintLayout>